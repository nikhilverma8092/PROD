@model IEnumerable<KYN_App_v1._1.Models.ResponseBusinessPrimaryDetailViewModel>


<h2>Search Results By SubCategory</h2>

<link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
<link type="text/css" rel="stylesheet" href="~/Promotional/materialize/css/materialize.min.css" media="screen,projection" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
<script type="text/javascript" src="~/Promotional/materialize/js/materialize.min.js"></script>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var grid = new WebGrid(Model);
}

<h2>Search Result</h2>
@{

    foreach (var item in Model)
    {
        <div class="col s12 m7">
            <h5 class="header">@item.Name</h5>
            <div class="card horizontal">
                <div class="card-image">
                    <img src="http://lorempixel.com/100/190/nature/6">
                </div>
                <div class="card-stacked">
                    <div class="card-content">
                        <div class="row">
                            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                                <p>@item.OfficialContactNumber1</p>
                                <p>@item.OfficialContactNumber1</p>
                                <p>@item.BusinessEmail</p>
                            </div>
                        </div>
                    </div>    
                </div>
            </div>
        </div>
    }
}

