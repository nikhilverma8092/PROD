{var orderId=$("#hdnCnclOrdrId").val();$.ajax({url:DOMAIN+"functions/cancelOrder.php",type:"post",data:{orderId:orderId,timeStamp:new Date().getTime()},success:function(res){$("#hdnCnclOrdrId").val('');closeDiv('cnclordr');window.location.reload();}});}
