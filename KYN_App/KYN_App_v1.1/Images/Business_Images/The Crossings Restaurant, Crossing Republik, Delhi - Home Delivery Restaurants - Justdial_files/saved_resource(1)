function timout(){auto=false;JSONS(timeoutURL,timeoutparameter,timeoutdivHolder);timeauto=null;}
var auto=true;var timeoutURL="";var timeoutparameter="";var timeoutdivHolder="";var timeauto="";function addRating(rate,type)
{if(!type)
type='ms';if(rate=='0.0')return;var tmp=rate.toString().split('.');var star='<span class="stars_m">';for(i=0;i<5;i++)
{if(i<tmp[0])
star+='<span class="ms10"></span>';else if(i==tmp[0]&&tmp[1])
star+='<span class="ms'+tmp[1]+'"></span>';else
star+='<span class="ms0"></span>';}
star+='</span>';return star;}
function getFormBy_name(formId){var formObj={};var inputs=$('form[name="'+formId+'"]').serializeArray();$.each(inputs,function(i,input){formObj[input.name]=input.value;});return formObj;}
function ReadCookie(key){var c=document.cookie;if(c.length!=0){var val=c.match('(^|;)[\\s]*'+key+'=([^;]*)');return val===null?'':decodeURIComponent(val[2]);}
return'';}
function doWriteCookie(key,val,expires_secs){var cvalu=ReadCookie(key);if(cvalu==null||cvalu==''||cvalu=='undefined'||cvalu==undefined||!cvalu){var c=key+'='+val+';path=/;',d=new Date();if(expires_secs!==null){d.setTime(d.getTime()+(1000*expires_secs));c+='expires='+d.toGMTString()+';';}
if(expires_secs!==null)
c+='max-age='+expires_secs+';';document.cookie=c;return 1;}else{return 0;}}
