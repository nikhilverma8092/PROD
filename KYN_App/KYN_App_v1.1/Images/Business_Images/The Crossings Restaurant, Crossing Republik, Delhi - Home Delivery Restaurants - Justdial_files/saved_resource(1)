function suffleButton(){$(".cntanr").find(".book-btn").each(function(){var btn_len=$(this).find("a").length;var btn_arr=[];var btn=[];var btn_other=[];var final_btn=[];if(btn_len>2){$(this).find("a").each(function(){btn_arr.push($(this).attr("title"));});if(btn_arr.indexOf("Shop Online")>-1){btn.push("Shop Online");}else{var y=btn_arr[Math.floor(Math.random()*btn_arr.length)];btn.push(y);}
var removeItem="Shop Online";btn_other=$.grep(btn_arr,function(value){return value!=removeItem;});var x=btn_other[Math.floor(Math.random()*btn_other.length)];if(x===y){var z=btn_other[Math.floor(Math.random()*btn_other.length)];btn.push(z);}else{btn.push(x);}
$(this).find("a").each(function(){if(btn.indexOf($(this).attr("title"))>-1){}else{$(this).hide();}});}});}
