function amentiesList(amnetyObj){var amnArr=(amnetyObj.toString()).split(",");var lenAmn=amnArr.length;var noUL=Math.ceil(lenAmn/20);var ulWidth="23%";var amnList="<div style='width:100%' class='amnDiv'>";amnList+="<ul><li class='promoLi'>Room Amenities</li></ul>";var amn=0;var amnLength=(lenAmn>=20)?20:lenAmn;for(var amnUL=0;amnUL<noUL;amnUL++){amnList+="<ul class='incLi'>";amnLength=(((amnUL+1)*20)<=lenAmn)?(amnUL+1)*20:lenAmn;for(;amn<amnLength;amn++){amnList+="<li>"+amnArr[amn]+"</li>";}
amnList+="</ul>";}
