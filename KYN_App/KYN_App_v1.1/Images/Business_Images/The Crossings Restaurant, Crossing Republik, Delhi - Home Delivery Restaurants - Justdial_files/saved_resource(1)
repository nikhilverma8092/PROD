{mat=kval[3].match(/\((.*)\)/);if(mat!==null&&mat!==undefined&&mat!==''&&mat!=='null'&&mat!=='undefined'&&typeof mat!=='undefined')
{filtername=kval[3].replace(/-/g,' ');filtername=filtername.replace('To','-');filtername=filtername.replace('Below','Less than');filtername=filtername.replace(/Rs/g,'<span class="rs"></span>');filtername=filtername.split('(');filtername=filtername[1];filtername=filtername.replace(')','');}}
if(kval[1].indexOf("(")!==-1)
{k=kval[1].split("(");if(k[1]!==null&&k[1]!==undefined&&k[1]!==''&&typeof k[1]!=='undefined')
{kval[1]=k[0]+" ("+k[1];}
else
{kval[1]=k[0];}}
priceRating=0;switch(kval[1])
{case'Inexpensive':priceRating=1;break;case'Moderate':priceRating=2;break;case'Expensive':priceRating=3;break;case'Very Expensive':priceRating=4;break;default:priceRating=0;}
price_rating=getProperPriceStars(priceRating);rupeeFilterUrl=DOMAIN+city+"/"+kval[3]+res_ar+"/ct-"+kval[0];if(catid==kval[0]){var classname='act';}else{var classname='';}
