function feedbackRes()
{var alphaExp=/^[A-Za-z\s]+$/;var fbnamid=$("#fbnamid").val();var fbmobileid=$("#fbmobile").val();var email_id=$('#fbemail').val();var emailExp=/^(([0-9a-zA-Z]+[-._+&])*[0-9a-zA-Z]+@([-0-9a-zA-Z]+[.])+[a-zA-Z]{2,7}){0,1}$/;var mobExp=/^[7,8,9]{1}[0-9]{9}$/;var fbcomment=$("#fbcomment").val();var fbcity='Mumbai';if($("#ctlnk").is(':visible'))
{fbcity=$("#ctlnk").html();}
else
{fbcity=getCookie('inweb_city');}
if(fbnamid==""){$("#err_fbnamid").html("Name is compulsary");$("#err_fbnamid").css('color','#FF0000');$("#fbnamid").focus();$("#fbnamid").val("");$("#err_fbnamid").show();return false;}
