function addMrFrndSms()
{var validmobile=1;var mobileExp=/^[7,8,9]{1}[0-9]{9}$/;var i,fName;var isValidInfo=true;var mobArr=new Array();$('#frndSMSErr').html('');$('#frndSMSErr').hide();$('[id^="shrmnm"]').each(function(){fName=$(this).attr('id');i=fName.replace('shrmnm','');FrndName=$('#shrmnm'+i).val();FrndMobile=$('#shrmn'+i).val();FrndName=trim(FrndName);FrndMobile=trim(FrndMobile);if(FrndName==''||FrndName.search('e.g')==0)
{isValidInfo=true;}
