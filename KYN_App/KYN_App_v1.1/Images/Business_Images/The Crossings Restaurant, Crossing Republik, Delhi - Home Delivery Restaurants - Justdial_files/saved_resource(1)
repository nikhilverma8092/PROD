function showIMP(text){var showText="";showText+='<div onclick="close_cp();" class="cross_css">X</div>';showText+=text;$("#showIncMp").html(unescape(showText));$("#showIncMp").show();$("#fade").show();}
function cancelP(){$("#showCp").show();$("#fade").show();$('html, body').css({'overflow':'hidden'});}
function close_cp(){$('#showCp').hide();$('#showIncMp').hide();$(".gdsIncl").hide();$("#fade").hide();$('html, body').css({'overflow':'visible'});}
function moreLess(roomWiseIncl){var moretext=" More";var showChar=10;var content=roomWiseIncl;var c=content;var para=c.split(' ');var len=para.length;if(len>showChar){var c='';var para1Len=25;if(Math.ceil(len/2)>25){para1Len=25;}else{para1Len=Math.ceil(len/2);}
for(var x=0;x<para1Len;x++){c+=para[x]+' ';}}
return c;}
function moreLessM(rMpStr){var moretext=" More";var c=rMpStr;var showChar=10;var para=c.split(' ');var len=para.length;if(len>showChar){var para1Len=25;if(Math.ceil(len/2)>25){para1Len=25;}else{para1Len=Math.ceil(len/2);}
for(var x=0;x<para1Len;x++){c+=para[x]+' ';}}
return c;}
function bookNow(pc,rc){var para={};para.action="log";para.searchID=SID;para.RC=rc;para.PC=pc;para.HID=hotelID;$('a[data-id="rcBN'+rc+'"]').attr("href",VERTICAL_DOMAIN+"hotel/login?action=log&searchID="+SID+"&RC="+encodeURI(rc)+"&PC="+encodeURI(pc)+"&HID="+hotelID);}
function showGdsIns(ref_no,hI,rC,pC){disabled=true;$.ajax({type:"GET",url:HBWrapperAPI+'api/gdsFeed.php?reference_number='+encodeURI($.trim(ref_no))+'&hotel_id='+encodeURI($.trim(hI))+'&room_code='+encodeURI($.trim(rC))+'&plan_code='+encodeURI($.trim(pC)),beforeSend:function(){$('a[data-id="gdsInc'+pC+'"]').html('<img src="'+IMG_DOMAIN+'blueLoad.png" alt="Loading Data..." style="margin:10px 0 0 20px;">');$('a[data-id="gdsInc'+pC+'"]').html('<img src="'+IMG_DOMAIN+'blueLoad.png" alt="Loading Data..." style="margin:10px 0 0 20px;">');},success:function(data){if(data){$('a[data-id="gdsInc'+pC+'"]').html(data);$('a[data-id="gdsInc'+pC+'"]').css({"color":"#747474"});}}});}
