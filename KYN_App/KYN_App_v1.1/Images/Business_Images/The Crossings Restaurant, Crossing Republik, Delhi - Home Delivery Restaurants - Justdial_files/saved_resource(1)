function ratingready()
{if(navigator.userAgent.indexOf("iPad")==-1&&navigator.userAgent.indexOf("os 8")==-1)
{$('.undone').hover(function(){var currrt=($(this).parent().hasClass('dptr1'))?'WRTrating':'rating';$(this).prevAll().andSelf().removeClass('rhvr');var rel_att=$(this).attr('rel');if($('#'+currrt).val()=="")
{if(rel_att==1)
mo_text='Poor';else if(rel_att==2)
mo_text='Average';else if(rel_att==3)
mo_text='Good';else if(rel_att==4)
mo_text='Very Good';else if(rel_att==5)
mo_text='Excellent';if($('#rtText').length>0)
{$('#rtText').html('You are rating this as <b><i>"'+mo_text+'"</b></i>');}}
if($(this).parent().hasClass('dptr1')&&$('#dprtng1'))
{$('#dprtng1').html(rel_att);}else if($('#dprtng'))
{$('#dprtng').html(rel_att);}
$(this).prevAll().andSelf().addClass('rhvr');$(this).prevAll().andSelf().removeClass('done');$(this).nextAll().andSelf().removeClass('done');},function(){var currrt=($(this).parent().hasClass('dptr1'))?'WRTrating':'rating';$(this).prevAll().andSelf().removeClass('rhvr');if($('#'+currrt).val()!="")
{if($(this).parent().hasClass('dptr1')||$(this).parent().hasClass('stq_m'))
{for(var i=0.5;i<=$('#'+currrt).val();i+=0.5)
{if($(this).parent().hasClass('dptr1'))
{$('.dptr1 .ed'+i*2).addClass('done');}else{$('.stq_m .ed'+i*2).addClass('done');}}}else
{for(var i=1;i<=$('#'+currrt).val();i++)
{if($(this).attr('data-id')=='rtn')
{$('.ed'+i).addClass('done');}}}}
if($('#'+currrt).val()!="")
{if(tab!='writereview_movie'&&tab!='writereview_product'&&onloadFn=="detailsPage")
{if(!$(this).parent().hasClass('dptr1')&&!$(this).parent().hasClass('stq_m'))
{for(var i=0.5;i<=$('#'+currrt).val();i+=0.5)
{$('.edit'+i*2).addClass('done');}}
if(currrt=='WRTrating')
{$('#dprtng1').html($('#'+currrt).val());}else
{$('#dprtng').html($('#rating').val());}}else
{for(var i=1;i<=$('#'+currrt).val();i++)
{$('.edit'+i).addClass('done');}}}else
{if(tab!='writereview_movie'&&tab!='writereview_product'&&onloadFn=="detailsPage")
{if($(this).parent().hasClass('dptr1')&&$('#dprtng1'))
{$('#dprtng1').html('-');}else if($('#dprtng'))
{$('#dprtng').html('-');}}
if($('#rtText4prod').val())
var rtm=$('#rtText4prod').val();else
var rtm=$('#rtText4movie').val();if(rtm!="movie"&&rtm!="product")
rtm="establishment";if($('#rtText').length>0)
{$('#rtText').html('Click on the blocks to rate this '+rtm+'...');}}});}
$('.undone').click(function(){$('#ratingErr').html("");var r_text="";$(this).prevAll().andSelf().removeClass('rhvr');var userAgent=navigator.userAgent.toUpperCase();if(userAgent.indexOf("IPAD")!=-1)
{$('.rstrs a').removeClass('done');}
$(this).prevAll().andSelf().addClass('done');var rate_val=$(this).attr('rel');if($(this).parent().hasClass('dptr1')&&$('#dprtng1'))
{$('#dprtng1').html(rate_val);}else if($('#dprtng'))
{$('#dprtng').html(rate_val);}
