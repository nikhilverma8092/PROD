if(modifiy==1)
{var ref_num=reference_number;$.getJSON(DOMAIN+"functions/ajxbooktable.php",{btcid:btcid,btname:btname,btmobile:btmobile,btdate:btdate,bttime:bttime,btnoprsn:btnoprsn,vcode:vcode,bttype:bttype,btmin_time_rsvn:btmin_time_rsvn,btemail:btemail,reference_number:ref_num,bcapacity:bcapacity,slot:slot,modifiy:modifiy},function(data)
{$('.err').html('');$('.err').addClass('dn');var flag=data.resultsdata[0].flag;var errorcode=data.resultsdata[0].error_code;var errormsg=data.resultsdata[0].error_msg;if(flag==8)
