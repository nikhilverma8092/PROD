{FrndName='User';fnmstr+=FrndName+'##'+FrndMobile+',';isValidInfo=true;}else
{fnmstr+=FrndName+'##'+FrndMobile+',';}
if(!(/^[a-zA-Z]+([.]{0,1}[ ]{0,1}[a-zA-Z]+)*$/.test(FrndName)))
{isValidInfo=false;}
if(FrndMobile=='')
{isValidInfo=false;}
if(!($.trim(FrndMobile).match(mobileExp)))
{isValidInfo=false;}
if(FrndMobile!=''){if($.inArray(FrndMobile,mobArr)==-1)
{mobArr.push(FrndMobile);}else
{$("#frndSMSErr").html("Duplicate mobile entered");$("#frndSMSErr").show();$('#shrmn'+i).val('');$('#shrmn'+i).focus();validmobile=0;return false;}}});if(isValidInfo==false){$('#frndSMSErr').html('Please enter valid mobile number');$('#frndSMSErr').show();validmobile=0;return false;}
