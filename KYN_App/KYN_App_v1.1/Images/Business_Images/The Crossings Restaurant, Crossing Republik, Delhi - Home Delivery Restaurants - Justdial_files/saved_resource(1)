{var vbtmobile=getCookie('inLogMobile');var inClienUID=getCookie('inClienUID');if(vbtmobile!=undefined&&vbtmobile!=''&&inClienUID!=''&&inClienUID!=undefined)
{$.ajax({url:DOMAIN+"functions/ajxrsvndetails.php",type:"GET",dataType:"json",data:{'mobile':vbtmobile,'docid':docid,'type':'checkowner'},success:function(result)
{if(result==1)
{var now=new Date();var time=now.getTime();time+=3600*1000;now.setTime(time);document.cookie='vendorpop='+escape(1)+'; expires='+now.toGMTString()+'; path=/'+'; domain='+cookieondomain;window.location=DOMAIN+"Account/Table-Rsvn?vndr=1";}
else
{bookAtable('',docid);}}});}
else
{bookAtable('1',docid);}}
