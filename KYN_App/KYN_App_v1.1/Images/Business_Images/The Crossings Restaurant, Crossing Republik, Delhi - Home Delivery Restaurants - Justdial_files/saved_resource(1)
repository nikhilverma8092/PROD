if(!fbnamid.match(alphaExp)){$("#err_fbnamid").html("Please enter a valid name");$("#err_fbnamid").css('color','#FF0000');$("#fbnamid").focus();$("#fbnamid").val("");$('#err_fbnamid').show();return false;}
if(fbnamid!=""){$("#err_fbnamid").html("");$('#err_fbnamid').hide();}
if((fbmobileid!=''&&fbmobileid.match(mobExp)==null)||fbmobileid==9867045061){$("#err_fbmobile").html("Please enter a valid mobile");$("#err_fbmobile").css('color','#FF0000');$("#fbmobile").focus();$("#fbmobile").val("");$('#err_fbmobile').show();return false;}
else{$("#err_fbmobile").html("");$("#err_fbmobile").hide();}
if(!emailExp.test(email_id)&&email_id!=""){$("#err_fbemail").html("Please enter a valid email id.");$("#err_fbemail").css('color','#FF0000');$("#fbemail").focus();$("#fbemail").val("");$('#err_fbemail').show();return false;}
if(email_id!=""){$("#err_fbemail").html("");$('#err_fbemail').hide();}
if(fbcomment==""){$("#err_fbcomment").html("Comment is compulsary");$("#err_fbcomment").css('color','#FF0000');$("#fbcomment").focus();$("#fbcomment").val("");$('#err_fbcomment').show();return false;}
else{$("#err_fbcomment").html("");$("#err_fbcomment").hide();}
