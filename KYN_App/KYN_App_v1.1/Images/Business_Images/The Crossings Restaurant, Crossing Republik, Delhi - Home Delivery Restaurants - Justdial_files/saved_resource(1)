function stripslashes(str){return(str+'').replace(/\\(.?)/g,function(s,n1){switch(n1){case'\\':return'\\';case'0':return'\u0000';case'':return'';default:return n1;}});}
function getProperStars(rating)
{star_string='';temp=rating.split('.');int_part=temp[0].trim();half=false;float_part=$temp[1].trim().substr(0,1);star_string+="<span class='stars_m'>";for(var i=1;i<=int_part;i++)
{star_string+="<span class='ms10'></span>";}
if(float_part>=1)
{star_string+="<span class='ms"+$float_part+"'></span>";half=true;}
if(half)
blank=4-int_part;else
blank=5-int_part;for(var i=1;i<=blank;i++)
{star_string+="<span class='ms0'></span>";}
star_string+="</span>";return star_string;}
function getProperPriceStars(rating)
{var rupee_arr=new Array();if(rating>0){for(var i=0;i<4;i++)
{if(i<rating)
rupee_arr[i]='rp0';else
rupee_arr[i]='rp1';}}
return rupee_arr;}
function getProperStars_movies(rating,selected)
{star_string='';temp=rating.split('.');int_part=temp[0].trim();half=false;float_part=temp[1].trim();float_part=float_part.substr(0,1);star_string+="<span class='stars_m'>";if(selected==1)
cssclass='mg';else if(selected==2)
cssclass='ms';else if(selected==3)
cssclass='rs';else
cssclass='mw';for(var i=1;i<=int_part;i++)
{star_string+="<span class='"+cssclass+"10'></span>";}
if(float_part>=1)
{star_string+="<span class='"+cssclass+float_part+"'></span>";half=true;}
if(half)
blank=4-int_part;else
blank=5-int_part;for(var i=1;i<=blank;i++)
{star_string+="<span class='"+cssclass+"0'></span>";}
star_string+="</span>";return star_string;}
