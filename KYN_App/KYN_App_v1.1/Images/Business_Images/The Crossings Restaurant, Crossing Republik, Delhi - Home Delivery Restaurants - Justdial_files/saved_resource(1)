{emlArr.push(FrndEmail);}else
{$("#frndErr").html("Duplicate email entered");$("#frndErr").show();$('#shrem'+i).val('');$('#shrem'+i).focus();validemail=0;return false;}}});if(isValidInfo==false){$('#frndErr').html('Please enter valid email id');$('#frndErr').show();validemail=0;return false;}
