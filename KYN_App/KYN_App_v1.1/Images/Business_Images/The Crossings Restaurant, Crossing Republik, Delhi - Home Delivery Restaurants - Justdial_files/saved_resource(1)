{$('#rating').val(rate_val);}
if(tab!='writereview'&&tab!='updatereview'&&tab!='writereview_movie'&&tab!='writereview_product'&&onloadFn=="detailsPage")
{if($(this).attr('data-id')=='WRT')
{}else{_ct('sbmtrtng',lnk_loc);submitreview_detail(0,'','');}}}
if(rate_val==1)
r_text='Poor';else if(rate_val==2)
